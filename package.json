{
  "scripts": {
    "dev": "ts-node-dev ./src/server.ts",
    "db:create": "npx sequelize db:create --config ./src/database/config/config.json",
    "migrations:run": "npx sequelize db:migrate --config ./src/database/config/config.js --migrations-path ./src/database/migrations/",
    "migrations:revert": "npx sequelize db:migrate:undo --config ./src/database/config/config.js --migrations-path ./src/database/migrations/"
  },
  "dependencies": {
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.1",
    "@types/sequelize": "^4.28.13",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
